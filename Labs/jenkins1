pipeline{
    agent any
    environment{
        IMAGE_NAME = 'myhttpd'
        VERSION = 'V1'
    }
    stages{
        stage("Docker_ver1"){
            steps{
                sh 'docker --version'
            }
        }
        stage("docker-build"){
            steps{
                dir('Labs'){
                  sh 'docker build -t ${IMAGE_NAME}:${VERSION} .'  
                }

                
            }
        }
        
    }
}